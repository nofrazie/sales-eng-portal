<div class="row">
  <div class="col-md-8">
    <table class="table table-bordered table-hover" id="integration_types">
      <thead>
        <tr>
          <th>Integraion Types</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <%= render @integration_types %>
      </tbody>
      <tbody>
        <%= form_for(@integration_type, remote: true) do |f| %>
          <tr>
            <td>
              <%= f.text_field :name %>
            </td>
            <td>
              <%= f.submit "Add", class: 'btn btn-xs btn-default', id: "integration-type-link" %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
